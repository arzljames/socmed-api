{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@util/jsonwebtoken/jsonwebtoken/(.*)": "<rootDir>/../libs/jsonwebtoken/src/$1",
    "@util/jsonwebtoken/jsonwebtoken": "<rootDir>/../libs/jsonwebtoken/src"
  }
}